package aula13;

/**
 *
 * @author pb948
 */
public abstract class CartaoWeb {
    String destinatario;
    
    public abstract String retornarMensagem(String remetente);

    public String getDestinatario() {
        return destinatario;
    }

    public void setDestinatario(String destinatario) {
        this.destinatario = destinatario;
    }
    
    
}

