   95  more /home/aluno/aulaSo/teste3.txt
   96  echo /home/aluno/aulaSo/teste1.txt /home/aluno/aulaSo/teste3.txt/
   97  echo 'quinta feira' /home/aluno/aulaSo/teste1.txt
   98  echo quinta feira >> /home/aluno/aulaSo/teste3.txt
   99  more /home/aluno/aulaSo/teste3.txt
